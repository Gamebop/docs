{
    "compilerOptions": {
        "allowJs": true,
        "checkJs": false,
        "lib": [
            "es2019",
            "dom",
            "dom.iterable"
        ],
        "target": "es6",
        "moduleResolution": "node",
        "noImplicitReturns": true,
        "noImplicitAny": true
    },
    "include": [ "submodules/physics/src/**/*.mjs" ],
    "typedocOptions": {
        "entryPoints": [
            "submodules/physics/src/index.mjs"
        ],
        "exclude": [
            "**/node_modules/**"
        ],
        "hostedBaseUrl": "https://gamebop.github.io/docs/",
        "navigation": {
            "includeCategories": true,
            "includeGroups": true
        },
        "categorizeByGroup": true,
        "excludeNotDocumented": true,
        "name": "Physics Components",
        "out": "docs",
        "readme": "none"
    }
}